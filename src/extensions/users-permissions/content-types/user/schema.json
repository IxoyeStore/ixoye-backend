{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users"
    },

    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "motherLastName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "birthDate": {
      "type": "date"
    }
  }
}
