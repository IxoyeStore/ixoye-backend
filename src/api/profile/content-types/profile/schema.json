{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "motherLastName": {
      "type": "string"
    },
    "phone": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "birthDate": {
      "type": "date"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "default": "b2c",
      "enum": [
        "b2c",
        "b2b"
      ]
    },
    "companyName": {
      "type": "string"
    }
  }
}
